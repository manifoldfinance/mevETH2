{
  "name": "mev3",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "files": [
    "/dist/**/*",
    "/contracts",
    "/contracts/base",
    "/contracts/interfaces",
    "/contracts/libraries",
    "/artifacts/contracts/**/*.json",
    "!artifacts/contracts/**/*.dbg.json",
    "!artifacts/contracts/test/**/*",
    "!artifacts/contracts/base/**/*"
  ],
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test",
    "contracts": "src"
  },
  "scripts": {
    "build": "forge build",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage",
    "forge-typechain": "npx typechain --discriminate-types --target ethers-v6 \"out/**/*.json\"",
    "test": "forge test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "lint": "npx gts lint",
    "fmt": "npx prettier --config prettier.config.js --write contracts/",
    "clean": "echo 'cleaning up' && rm -rf ./artifacts && rm -rf ./dist && rm -rf ./coverage && rm -rf ./build && rm -rf ./cache && rm -rf ./reports && rm -rf ./types && rm -rf ./coverage.json && rm -rf ./hardhat.config.ts && rm -rf ./tsconfig.json && rm -rf ./ts",
    "fix": "npx gts fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifoldfinance/meveth.git"
  },
  "keywords": [
    "solidity",
    "ethereum",
    "smart contracts",
    "defi"
  ],
  "bugs": {
    "url": "https://github.com/manifoldfinance/meveth/issues"
  },
  "author": "SEE CONTRIBUTORS",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.2",
    "forge-std": "git+https://github.com/foundry-rs/forge-std#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "safe-tools": "git+https://github.com/0xKitsune/safe-tools#42fa7ec87264590f7a00633f9e2d4d6e05db6d42",
    "solady": "git+https://github.com/Vectorized/solady#v0.0.107",
    "solmate": "git+https://github.com/transmissions11/solmate#bfc9c25865a274a7827fea5abf6e4fb64fc64e6c"
  },
  "volta": {
    "node": "18.16.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ethers": "^6.6.3",
    "hardhat": "^2.17.0",
    "hardhat-deploy": "^0.11.29",
    "hardhat-tracer": "^2.5.1",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typechain": "^8.2.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.0.2"
  }
}
